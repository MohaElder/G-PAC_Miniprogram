<view class="page-body">
 

 <view class="page__hd">
        <view class="page__title">11TH GRADE</view>
        <view class="page__desc">Announcements: Good luck on your SAT! </view>
    </view>
  <view class="page-section">

   <!--MathSection-->
  <view name = "Math Section">
      <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_input">
                <view class="weui-cell__bd">Math</view>
          <input class="weui-input" type="number" placeholder="Score..." bindinput = "getSubAScore" name = "SubAScore"/>
        </view>
      </view>
      <picker bindchange="getSubALevel"  value="{{SubAindex}}" range="{{level}}">
        <view class="picker enhanceView">
          Level = {{level[SubAindex]}}
        </view>
      </picker>
        </view>

