<!--rank.wxml-->

<view class="cu-bar bg-white">
  <view class='action'>
    <text class='icon-titles text-pink'></text>
    <text class='text-xl text-bold'>Me</text>
  </view>
</view>

<!--<scroll-view scroll-y style="height: 1000px;" scroll-into-view="{{toView}}" scroll-top="{{scrollTop}}" aria-label="true" enable-back-to-top="true">-->

<view name = "profileSection">
  <view class="cu-card dynamic">
    <view wx:for="1" wx:key class="cu-item shadow">
      <view class="cu-list menu menu-avatar">
        <view class="cu-item">
         <image class='cu-avatar round lg' src='{{RankPic}}'/>
          <view class='content flex-sub'>
            <view>{{Name}}</view>
            <view class='text-gray text-sm flex justify-between'>
              {{brief}}
            </view>
          </view>
        </view>
      </view>

      <view class="cu-list menu menu-avatar comment solids-top">
        <view class="cu-item" wx:for="{{1}}" wx:key>
          <view class='content'>
            <view class='text-grey'>   I defeated {{Defeat}}% of the users this time.</view>
          </view>
        </view>
      </view>

          <view class="cu-list menu menu-avatar comment solids-top">
        <view class="cu-item" wx:for="{{1}}" wx:key>
          <view class='content'>
            <view class='text-grey'>   I am in Grade {{Grade}}</view>
          </view>
        </view>
      </view>

              <view class="cu-list menu menu-avatar comment solids-top">
        <view class="cu-item" wx:for="{{1}}" wx:key>
          <view class='content'>
            <view class='text-grey'>   My GPA is : {{GPA}}</view>
          </view>
        </view>
      </view>

                  <view class="cu-list menu menu-avatar comment solids-top">
        <view class="cu-item" wx:for="{{1}}" wx:key>
          <view class='content'>
            <view class='text-grey'>   I am qualified as {{RankName}}</view>
          </view>
        </view>
      </view>

    </view>
  </view>
</view>

  <ec-canvas wx:if="{{!isDisposed}}" id="mychart-dom-move-bar" canvas-id="mychart-move-bar" ec="{{ ec }}"></ec-canvas>

<!--</scroll-view>-->

<view name = "TimelineSection">
  <view class="cu-card dynamic">
    <view wx:for="1" wx:key class="cu-item shadow">
        <view class="cu-item">
<view class="cu-timeline" >
  <view class="cu-time">Your GPAs</view>

  <view wx:for="{{historyList}}" class='cu-item text-blue' >
    <view class="bg-blue content"  wx:key>
      Your got a GPA of {{item}} at {{item.time}}.
    </view>
  </view>
</view>
        </view>
      </view>
</view>
</view>

