<!--rank.wxml-->

<scroll-view scroll-y style="height: 1000px;" scroll-into-view="{{toView}}" scroll-top="{{scrollTop}}" aria-label="true" enable-back-to-top="true">

<view class="cu-card dynamic {{isCard?'no-card':''}}">
  <view wx:for="1" wx:key class="cu-item shadow">
    <view class="cu-list menu menu-avatar">
      <view class="cu-item">
        <view class="cu-avatar round lg" style="background-image:url(https://image.weilanwl.com/img/square-1.jpg);"></view>
        <view class='content flex-sub'>
          <view>UserName</view>
          <view class='text-gray text-sm flex justify-between'>
            2018年12月3日
          </view>
        </view>
      </view>
    </view>
    <view class='text-content'>
      Anything you wanna add here
    </view>

    <view class="cu-list menu menu-avatar comment solids-top">
      <view class="cu-item" wx:for="{{1}}" wx:key>
        <view class='content'>
          <view class='text-grey'>   I defeated </view>
          <view class='text-gray text-content text-df'>
               {{Defeat}}% of the users, but does that really matter?
          </view>
        </view>
      </view>
    </view>

        <view class="cu-list menu menu-avatar comment solids-top">
      <view class="cu-item" wx:for="{{1}}" wx:key>
        <view class='content'>
          <view class='text-grey'>   I am in Grade {{Grade}}</view>
        </view>
      </view>
    </view>

            <view class="cu-list menu menu-avatar comment solids-top">
      <view class="cu-item" wx:for="{{1}}" wx:key>
        <view class='content'>
          <view class='text-grey'>   My GPA is : {{GPA}}</view>
        </view>
      </view>
    </view>

                <view class="cu-list menu menu-avatar comment solids-top">
      <view class="cu-item" wx:for="{{1}}" wx:key>
        <view class='content'>
          <view class='text-grey'>   I am qualified as {{RankName}}</view>
        </view>
      </view>
    </view>

  </view>
</view>




       <view class="container">
     <ec-canvas wx:if="{{!isDisposed}}" id="mychart-dom-scatter" canvas-id="mychart-scatter" ec="{{ ec }}"></ec-canvas>
  </view>
  
</scroll-view>