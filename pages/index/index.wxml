<!--index.wxml-->
<view class="container">
  <view class="userinfo">
      <image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image>
      <text class="userinfo-nickname">Welcome! {{userInfo.nickName}}</text>
  </view>
  <view class="usermotto">
    <van-button bindtap = "To8">8th</van-button>
     <van-button bindtap = "To9">9th</van-button>
      <van-button bindtap = "To10">10th</van-button>
       <van-button bindtap = "To11">11th</van-button>
  </view>
</view>
