<view class="page-body">

 <view class="page__hd">
        <view class="page__title">
        <picker bindchange="changePreset" value="{{presetIndex}}" range="{{presetList}}">
        <view class="picker">
          You are at: {{presetList[presetIndex]}}
        </view>
      </picker>
      </view>
        <view class="page__desc">{{announcement}}</view>
    </view>
  <view class="page-section">

<view class="weui-cells weui-cells_after-title" wx:for="{{subjects}}">
  <view class="weui-cell weui-cell_input">
    <view class="weui-cell__bd">{{item.subjectName}}</view>
     <input class="weui-input" type="number" placeholder="Score..." bindinput = "getSubScore" data-index = "{{index}}"/>
  </view>
      <picker bindchange="getSubLevel" value="{{item.selectedValue}}" data-index = "{{index}}" range="{{item.level}}">
        <view class="picker enhanceView">
          Level = {{item.level[item.selectedValue]}}
        </view>
      </picker>
</view>

<button class="weui-btn enhanceButton" type="primary" open-type="getUserInfo" bindgetuserinfo='userInfo'>Submit</button>
</view>
</view>
